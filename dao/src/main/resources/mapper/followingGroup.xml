<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD MAPPER 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.chenyangqi.dao.FollowingGroupDao">

    <select id="getByType" parameterType="java.lang.String" resultType="com.chenyangqi.domain.FollowingGroup">
        select *
        from t_following_group
        where 'type' = #{type}
    </select>

    <select id="getById" parameterType="java.lang.Integer" resultType="com.chenyangqi.domain.FollowingGroup">
        select *
        from t_following_group
        where id = #{id}
    </select>

</mapper>